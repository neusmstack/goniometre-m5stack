lock></statement></block></statement></block></next></block></next></block></statement></block></next></block></statement></block>","screen":[{"simulationName":"Built-in","type":"builtin","width":320,"height":240,"scale":0.78,"screenName":"","blockId":"","screenColorType":0,"id":"builtin","createTime":1749304253762}],"logicWhenNum":0,"pythonCode":"import os, sys, io\nimport M5\nfrom M5 import *\nfrom unit import CardKBUnit\nfrom hardware import I2C\nfrom hardware import Pin\nimport time\nfrom unit import IMUUnit\nfrom unit import Servos8Unit\nfrom unit import PAHUBUnit\n\n\n\ni2c0 = None\npahub_0 = None\ncardkb_0 = None\nservos8_0 = None\nimu_0 = None\n\n\nimport math\n\nangle_actual = None\nangle_servo = None\nangle_real = None\ninput_original = None\norientacio = None\npitch = None\ninput2 = None\npitch_calibrat = None\npas = None\nstring = None\npitch_offset = None\naltura_fixa_del_lector = None\nangle_radiant = None\naltura_calculada = None\naltura_real = None\nlongitud = None\ntext_cardkb = None\npitch_total = None\ni = None\n\n# Describe this function...\ndef update_pitch_calibrat():\n  global angle_actual, angle_servo, angle_real, input_original, orientacio, pitch, input2, pitch_calibrat, pas, string, pitch_offset, altura_fixa_del_lector, angle_radiant, altura_calculada, altura_real, longitud, text_cardkb, pitch_total, i, i2c0, pahub_0, cardkb_0, servos8_0, imu_0\n  angle_actual = angle_servo\n  orientacio = imu_0.get_attitude()\n  pitch = orientacio[0]\n  pitch_calibrat = pitch - pitch_offset\n  angle_real = pitch_calibrat\n  angle_servo = int(angle_real + 90)\n  servos8_0.set_servo_angle(int(angle_servo), 7)\n\n# Describe this function...\ndef mou_el_bra_C3_A7():\n  global angle_actual, angle_servo, angle_real, input_original, orientacio, pitch, input2, pitch_calibrat, pas, string, pitch_offset, altura_fixa_del_lector, angle_radiant, altura_calculada, altura_real, longitud, text_cardkb, pitch_total, i, i2c0, pahub_0, cardkb_0, servos8_0, imu_0\n  angle_servo = 90 + angle_real\n  if angle_servo < 0:\n    angle_servo = 0\n  elif angle_servo > 180:\n    angle_servo = 180\n  if angle_servo > angle_actual:\n    pas = 1\n  else:\n    pas = -1\n  while angle_actual != angle_servo:\n    angle_actual = angle_actual + pas\n    servos8_0.set_servo_angle(int(angle_actual), 7)\n    M5.Lcd.fillRect(10, 75, 320, 40, 0x990000)\n    M5.Lcd.drawRect(10, 75, 320, 40, 0x990000)\n    M5.Lcd.setCursor(20, 83)\n    M5.Lcd.setFont(Widgets.FONTS.DejaVu24)\n    M5.Lcd.print((str(((str('Angle real: ') + str((math.fabs(int(angle_real))))))) + str(' graus')), 0xffffff)\n    time.sleep(0.01)\n\n\ndef btnA_wasClicked_event(state):\n  global i2c0, pahub_0, cardkb_0, servos8_0, imu_0, angle_actual, angle_servo, angle_real, input_original, orientacio, pitch, input2, pitch_calibrat, pas, string, pitch_offset, altura_fixa_del_lector, angle_radiant, altura_calculada, altura_real, longitud, math, text_cardkb, pitch_total\n  angle_real = (angle_real if isinstance(angle_real, (int, float)) else 0) + -1\n  mou_el_bra_C3_A7()\n\n\ndef btnB_wasClicked_event(state):\n  global i2c0, pahub_0, cardkb_0, servos8_0, imu_0, angle_actual, angle_servo, angle_real, input_original, orientacio, pitch, input2, pitch_calibrat, pas, string, pitch_offset, altura_fixa_del_lector, angle_radiant, altura_calculada, altura_real, longitud, math, text_cardkb, pitch_total\n  angle_real = (angle_real if isinstance(angle_real, (int, float)) else 0) + 1\n  mou_el_bra_C3_A7()\n\n\ndef btnC_wasClicked_event(state):\n  global i2c0, pahub_0, cardkb_0, servos8_0, imu_0, angle_actual, angle_servo, angle_real, input_original, orientacio, pitch, input2, pitch_calibrat, pas, string, pitch_offset, altura_fixa_del_lector, angle_radiant, altura_calculada, altura_real, longitud, math, text_cardkb, pitch_total\n  Widgets.setRotation(3)\n  M5.Lcd.fillScreen(0xffffff)\n  M5.Lcd.startWrite()\n  M5.Lcd.setFont(Widgets.FONTS.DejaVu18)\n  M5.Lcd.setCursor(10, 40)\n  M5.Lcd.print('Longitud del laser: ', 0x006600)\n  M5.Lcd.fillRect(10, 75, 300, 40, 0xffffff)\n  M5.Lcd.setCursor(20, 83)\n  pas = 0\n  string = ''\n  input_original = 0\n\n\ndef cardkb_0_pressed_event(kb):\n  global i2c0, pahub_0, cardkb_0, servos8_0, imu_0, angle_actual, angle_servo, angle_real, input_original, orientacio, pitch, input2, pitch_calibrat, pas, string, pitch_offset, altura_fixa_del_lector, angle_radiant, altura_calculada, altura_real, longitud, math, text_cardkb, pitch_total\n  input_original = cardkb_0.get_key()\n  if input_original >= 0:\n    input2 = chr(input_original)\n    if input_original == 8 and len(string) > 0:\n      string = string.replace(string[-1], '')\n      M5.Lcd.fillRect(10, 115, 300, 30, 0xffffff)\n      M5.Lcd.setFont(Widgets.FONTS.DejaVu18)\n      M5.Lcd.setCursor(20, 120)\n      M5.Lcd.print(string, 0x000000)\n    else:\n      string = (str(string) + str(input2))\n      M5.Lcd.fillRect(10, 115, 300, 30, 0xffffff)\n      M5.Lcd.setFont(Widgets.FONTS.DejaVu18)\n      M5.Lcd.setCursor(20, 70)\n      M5.Lcd.print(string, 0x000000)\n    if input_original == 13 or input_original == 10:\n      if pas == 0:\n        M5.Lcd.fillRect(10, 20, 320, 105, 0xffffff)\n        M5.Lcd.fillRect(10, 115, 300, 30, 0xffffff)\n        longitud = float(string)\n        M5.Lcd.setFont(Widgets.FONTS.DejaVu18)\n        M5.Lcd.setCursor(10, 23)\n        M5.Lcd.print((str(((str('Longitud al punter: ') + str(string)))) + str(' m')), 0x000000)\n        pas = 1\n        string = ''\n        input_original = 0\n        M5.Lcd.setCursor(10, 100)\n        M5.Lcd.print('Valor del punter a terra en m ?', 0x000000)\n      elif pas == 1:\n        M5.Lcd.fillRect(10, 20, 320, 120, 0xffffff)\n        altura_fixa_del_lector = float(string)\n        M5.Lcd.setFont(Widgets.FONTS.DejaVu18)\n        M5.Lcd.setCursor(20, 100)\n        M5.Lcd.print((str(((str('Distancia terra al punter') + str(string)))) + str(' m')), 0x000000)\n        pas = 2\n        string = ''\n        input_original = 0\n        M5.Lcd.setFont(Widgets.FONTS.DejaVu18)\n        M5.Lcd.setCursor(20, 40)\n        M5.Lcd.print('Calculant angle en radiants...', 0x000000)\n      elif pas == 2:\n        M5.Lcd.fillRect(10, 20, 320, 120, 0xffffff)\n        angle_radiant = math.fabs(angle_real * math.pi) / 180\n        M5.Lcd.setFont(Widgets.FONTS.DejaVu18)\n        M5.Lcd.setCursor(20, 60)\n        M5.Lcd.print((str(((str('Angle en radiants: ') + str((round(angle_radiant, 4)))))) + str('rad')), 0x000000)\n        pas = 3\n        string = ''\n        input_original = 0\n        M5.Lcd.setCursor(20, 80)\n        M5.Lcd.print((str('Calculant altura...') + str(' en m')), 0x000000)\n      elif pas == 3:\n        M5.Lcd.fillRect(10, 20, 320, 120, 0xffffff)\n        altura_calculada = round(longitud * math.sin(angle_radiant), 3)\n        M5.Lcd.setFont(Widgets.FONTS.DejaVu18)\n        M5.Lcd.setCursor(20, 100)\n        M5.Lcd.print((str(((str('Altura calculada: ') + str(altura_calculada)))) + str(' m')), 0x000000)\n        string = ''\n        input_original = 0\n        pas = 4\n        M5.Lcd.setCursor(20, 120)\n        M5.Lcd.print('Calculant altura real... en m', 0x000000)\n      elif pas == 4:\n        M5.Lcd.fillRect(10, 20, 320, 120, 0xffffff)\n        altura_real = round(altura_calculada + altura_fixa_del_lector, 3)\n        M5.Lcd.setFont(Widgets.FONTS.DejaVu18)\n        M5.Lcd.setCursor(20, 140)\n        M5.Lcd.print((str(((str('Altura real: ') + str(altura_real)))) + str(' m')), 0x000000)\n        string = ''\n        input_original = 0\n\n\ndef setup():\n  global i2c0, pahub_0, cardkb_0, servos8_0, imu_0, angle_actual, angle_servo, angle_real, input_original, orientacio, pitch, input2, pitch_calibrat, pas, string, pitch_offset, altura_fixa_del_lector, angle_radiant, altura_calculada, altura_real, longitud, math, text_cardkb, pitch_total\n\n  M5.begin()\n  Widgets.fillScreen(0x222222)\n\n  BtnA.setCallback(type=BtnA.CB_TYPE.WAS_CLICKED, cb=btnA_wasClicked_event)\n  BtnB.setCallback(type=BtnB.CB_TYPE.WAS_CLICKED, cb=btnB_wasClicked_event)\n  BtnC.setCallback(type=BtnC.CB_TYPE.WAS_CLICKED, cb=btnC_wasClicked_event)\n\n  i2c0 = I2C(0, scl=Pin(22), sda=Pin(21), freq=100000)\n  servos8_0 = Servos8Unit(PAHUBUnit(i2c=i2c0, channel=4), 0x25)\n  imu_0 = IMUUnit(PAHUBUnit(i2c=i2c0, channel=1))\n  cardkb_0 = CardKBUnit(PAHUBUnit(i2c=i2c0, channel=3))\n  cardkb_0.set_callback(cardkb_0_pressed_event)\n  servos8_0.set_mode(3, 7)\n  angle_real = 0\n  angle_servo = (int(angle_real)) + 90\n  altura_calculada = 0\n  longitud = 0\n  angle_actual = 90\n  text_cardkb = ''\n  servos8_0.set_servo_angle(int(angle_servo), 7)\n  Widgets.setBrightness(150)\n  Widgets.fillScreen(0xffffff)\n  M5.Lcd.startWrite()\n  M5.Lcd.setCursor(10, 40)\n  M5.Lcd.setFont(Widgets.FONTS.DejaVu9)\n  M5.Lcd.print('Calibrant offset... ', 0x000000)\n  pitch_total = 0\n  for i in range(1, 6):\n    orientacio = imu_0.get_attitude()\n    pitch = orientacio[0]\n    pitch_total = pitch_total + pitch\n    time.sleep(0.2)\n  pitch_offset = pitch_total / 5\n  M5.Lcd.fillScreen(0xffffff)\n  M5.Lcd.startWrite()\n  M5.Lcd.setCursor(10, 40)\n  M5.Lcd.setFont(Widgets.FONTS.DejaVu9)\n  M5.Lcd.print((str('Pitch offset: ') + str((str(pitch_offset)))), 0x330099)\n  M5.Lcd.setFont(Widgets.FONTS.DejaVu9)\n  M5.Lcd.setCursor(10, 120)\n  M5.Lcd.print('Prem \"A\" per apujar el brac', 0x006600)\n  M5.Lcd.setCursor(10, 140)\n  M5.Lcd.print('Prem \"B\" per abaixar el brac', 0x006600)\n  M5.Lcd.setCursor(10, 160)\n  M5.Lcd.print('Prem \"C\" per introduir la longitud del punter laser: ', 0x006600)\n  string = ''\n  input2 = ''\n  input_original = 0\n  pas = 0\n  M5.Lcd.print('Introdueix la longitud del punter laser: ', 0x006600)\n  angle_actual = angle_servo\n\n\ndef loop():\n  global i2c0, pahub_0, cardkb_0, servos8_0, imu_0, angle_actual, angle_servo, angle_real, input_original, orientacio, pitch, input2, pitch_calibrat, pas, string, pitch_offset, altura_fixa_del_lector, angle_radiant, altura_calculada, altura_real, longitud, math, text_cardkb, pitch_total\n  M5.update()\n  time.sleep(0.05)\n  cardkb_0.tick()\n\n\nif __name__ == '__main__':\n  try:\n    setup()\n    while True:\n      loop()\n  except (Exception, KeyboardInterrupt) as e:\n    try:\n      from utility import print_error_msg\n      print_error_msg(e)\n    except ImportError:\n      print(\"please update to latest firmware\")\n","customList":[]}